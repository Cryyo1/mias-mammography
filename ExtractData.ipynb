    "import tarfile\n",
    "\n",
    "def extract_tar(fname, dest=\"./data/pgms\"):\n",
    "    mode = \"r:gz\" if (fname.endswith(\"tar.gz\")) else \"r:\"\n",
    "    tar = tarfile.open(fname, mode)\n",
    "    tar.extractall(path=dest)\n",
    "    tar.close()\n",
    "    \n",
    "extract_tar(\"./data/all-mias.tar.gz\")"
   ]
  }
